# API 설정
# 메인 API 엔드포인트
VITE_API_BASE_URL=https://your-api-url.com
VITE_WS_URL=wss://your-websocket-url.com

# AWS API Gateway 엔드포인트들
# 통합 API Gateway
VITE_API_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com
# 프롬프트 관리 API
VITE_PROMPT_API_URL=https://your-prompt-api-url.execute-api.region.amazonaws.com
# WebSocket API
VITE_WEBSOCKET_URL=wss://your-websocket-url.execute-api.region.amazonaws.com/prod
# 사용량 추적 API  
VITE_USAGE_API_URL=https://your-usage-api-url.execute-api.region.amazonaws.com/prod

# AWS Cognito 설정
# AWS 리전
VITE_AWS_REGION=us-east-1
# Cognito User Pool ID
VITE_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
# Cognito Client ID (앱 클라이언트 ID)
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxx

# External CDN
VITE_PDFJS_CDN_URL=https://cdnjs.cloudflare.com/ajax/libs/pdf.js

# 개발/테스트 설정
VITE_USE_MOCK=false